{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier",
    "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "align": false,
    "brace-style": false,
    "curly": false,
    "import-name": false,
    "jsx-boolean-value": false,
    "jsx-no-lambda": false,
    "max-line-length": [ true, "80" ],
    "no-duplicate-imports": true,
    "no-empty-pattern": false,
    "no-shadowed-variable": false,
    "no-unused-expression": false,
    "no-unused": true,
    "quotemark": [ false, "single" ],
    "semicolon": [ false, "never" ],
    "ter-arrow-parens": false,
    "ter-indent": false,
    "trailing-comma": true,
    "variable-name": false
  },
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**/*.ts"
    ]
  }
}